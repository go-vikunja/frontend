    "caniuse-lite": "1.0.30001540",
    "css-has-pseudo": "6.0.0",
    "csstype": "3.1.2",
    "cypress": "12.17.4",
    "esbuild": "0.19.3",
    "eslint": "8.50.0",
    "eslint-plugin-vue": "9.17.0",
    "happy-dom": "10.11.2",
    "histoire": "0.17.2",
    "postcss": "8.4.30",
